{
	"Lua.diagnostics.globals": [
		"Config",
		"log",
		"InGame"
	],
	"Lua.workspace.ignoreDir": [
		".git",
		".vscode",
		"Code-Server",
		"Obsidian Vault",
		"Packages",
		"PackageTemplate",
		"tools/Lua-Bundler",
		"**/Sim-Files"
	],
	"Lua.workspace.useGitIgnore": false,
	"Lua.workspace.ignoreSubmodules": false,
	"Lua.workspace.maxPreload": 10000,
	"Lua.workspace.preloadFileSize": 2000,
	"triggerTaskOnSave.tasks": {
		"check all": ["*"]
	},
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true,
		".stfolder": true,
		"Code-Server": true,
		"Obsidian Vault": true,
		"Packages": true
	},
	"asciidoc.antora.enableAntoraSupport": true,
	"cSpell.ignoreWords": [
		"attnamelist",
		"canbe",
		"commentstr",
		"elseifs",
		"explist",
		"foreq",
		"funcbody",
		"funcname",
		"functioncall",
		"getlinecol",
		"getpos",
		"gettokenthread",
		"gotos",
		"lastline",
		"laststat",
		"lasttoken",
		"lasttokentype",
		"mustbe",
		"namelist",
		"nexttoken",
		"nexttokentype",
		"patescape",
		"prefixexp",
		"readblock",
		"refreshparents",
		"retstat",
		"seekpast",
		"seekto",
		"tokentype",
		"vararg",
		"varlist"
	],
	"logFileHighlighter.customPatterns": [
		{
			"pattern": ".*\\[Log\\] ->.*",
			"foreground": "#1fffcb",
			"letterSpacing": "1.2px"
		},
		{
			"pattern": ".*\\[Trace\\]:.*",
			"foreground": "#969595",
			"letterSpacing": "1.2px"
		},
		{
			"pattern": ".*\\[Debug\\]:.*",
			"foreground": "#f8a632",
			"letterSpacing": "1.2px"
		},
		{
			"pattern": ".*\\[Info\\]:.*",
			"foreground": "#195eff",
			"letterSpacing": "1.2px"
		},
		{
			"pattern": ".*\\[Warning\\]:.*",
			"foreground": "#ffe921",
			"letterSpacing": "1.2px"
		},
		{
			"pattern": ".*\\[Error\\]:.*",
			"foreground": "#ff0000",
			"letterSpacing": "1.2px"
		},
		{
			"pattern": ".*\\[Fatal\\]:.*",
			"foreground": "#c400b7",
			"letterSpacing": "1.2px"
		},
		{
			"pattern": "\\[THREAD START\\]",
			"foreground": "#0da00d"
		}
	],
}
