{
	"Lua.workspace.ignoreDir": [
		".git",
		".vscode",
		"Code-Server",
		"Packages",
		"PackageTemplate",
		"tools/Lua-Bundler",
		"docu/tools",
		"**/Sim-Files",
		"**/Freemaker/src",
		"Tests",
		"Benchmarks"
	],
	"Lua.workspace.useGitIgnore": false,
	"Lua.workspace.ignoreSubmodules": false,
	"Lua.workspace.maxPreload": 10000,
	"Lua.workspace.preloadFileSize": 2000,
	"Lua.doc.privateName": ["m_*"],
	"triggerTaskOnSave.tasks": {
		"check all": ["*"]
	},
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true,
		"Code-Server": false,
		"Packages": true
	},
	"logFileHighlighter.customPatterns": [
		{
			"pattern": ".*\\[Log\\] ->.*",
			"foreground": "#1fffcb",
			"letterSpacing": "1.2px"
		},
		{
			"pattern": ".*\\[Trace\\] .*",
			"foreground": "#969595",
			"letterSpacing": "1.2px"
		},
		{
			"pattern": ".*\\[Debug\\] .*",
			"foreground": "#f8a632",
			"letterSpacing": "1.2px"
		},
		{
			"pattern": ".*\\[Info\\] .*",
			"foreground": "#2fdce2",
			"letterSpacing": "1.2px"
		},
		{
			"pattern": ".*\\[Warning\\] .*",
			"foreground": "#ffe921",
			"letterSpacing": "1.2px"
		},
		{
			"pattern": ".*\\[Error\\] .*",
			"foreground": "#ff0000",
			"letterSpacing": "1.2px"
		},
		{
			"pattern": ".*\\[Fatal\\] .*",
			"foreground": "#c400b7",
			"letterSpacing": "1.2px"
		},
		{
			"pattern": "\\[THREAD START\\]",
			"foreground": "#0da00d"
		}
	],
	"logFileHighlighter.enableProgressIndicator": true,
	"logFileHighlighter.progressIndicatorUnderlineColor": "#1eafc9",
	"Lua.diagnostics.globals": [
		"classes",
		"structs",
		"NotInGame"
	],
}
